{
  "name": "react-github-repos",
  "version": "1.0.0",
  "description": "A simple app that shows popular github repos in different languages. The app is built using Typescript, React and Webpack.",
  "author": "Salman Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/msalman86/react-github-battle"
  },
  "scripts": {
    "build": "npm run webpack -- --progress --profile",
    "clean:dist": "del-cli dist",
    "hot": "npm run server -- --hot",
    "prebuild": "npm run clean:dist",
    "server": "npm run webpack-dev-server -- --inline --profile --port 10000",
    "start": "npm run server",
    "watch": "npm run server -- --watch",
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "@types/query-string": "^4.3.1",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "@types/react-router-dom": "^4.0.4",
    "@types/redux-logger": "^3.0.0",
    "axios": "^0.16.1",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "del-cli": "^1.0.0",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.3.3",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
